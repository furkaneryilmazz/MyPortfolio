@model Message

@{
    ViewData["Title"] = "MessageDetail";
    Layout = "~/Views/_Layout/Index.cshtml";
	int count = 0;
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Tables</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Mesaj Detayları</div>
					</div>
					<div class="card-body">
						<div class="card-sub">
                            Gönderen: <strong>@Model.NameSurname - Email: @Model.Email - Tarih: @Model.SendDate</strong>
                            <br />
                            Mesaj: <strong>@Model.MessageDetail</strong>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

